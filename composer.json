{
    "name": "albert-modz/asignacion_de_horarios_uvz",
    "description": "Sistema de asignación de horarios mediante computación evolutiva para la Universidad del Valle sede Zarzal",
    "type": "project",
    "require": {
        "php": "^7.4 || ^8.0",
        "composer/composer": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ],
        "post-update-cmd": [
            "php artisan clear-compiled",
            "php artisan optimize"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    }
}
